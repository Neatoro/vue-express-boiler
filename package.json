{
  "name": "vue-boiler",
  "version": "0.0.1",
  "description": "A boilerplate code for a vue app",
  "main": "index.js",
  "repository": "git@github.com:Neatoro/vue-boiler.git",
  "author": "Moritz Schramm",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all build && node .",
    "dev": "npm-run-all build serve",
    "serve": "cross-env NODE_ENV=dev webpack-dev-server",
    "lint": "prettier-eslint --write \"src/**/*.js\" \"**/*.vue\"",
    "build": "npm-run-all build:js resources",
    "build:js": "webpack --mode=production",
    "resources": "cp -r src/resources/* public/"
  },
  "dependencies": {
    "dotenv": "8.0.0",
    "express": "4.17.1",
    "vue": "2.6.10",
    "vue-router": "3.0.6",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-loader": "8.0.6",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "4.7.1",
    "sass-loader": "7.1.0",
    "vue-loader": "15.7.0",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.32.2",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.5.1"
  }
}
